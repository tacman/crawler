{
  "name": "mediamonks/crawler",
  "type": "library",
  "description": "Crawl your own website with various clients for SEO and indexing purposes. ",
  "keywords": [
    "crawler",
    "seo",
    "dom",
    "spider",
    "robot",
    "prerender",
    "prerender.io",
    "search",
    "index",
    "goutte",
    "domcrawler"
  ],
  "homepage": "https://www.mediamonks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "fabpot/goutte": ">= 4.0",
    "league/uri": "^4.2",
    "psr/log": "^1.0 | ^2.0| ^3.0 ",
    "symfony/dom-crawler": ">= 5.4",
    "symfony/http-client": ">= 5.4"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "monolog/monolog": "^1.21",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^8.0",
    "rector/rector": "^0.13.8"
  },
  "autoload": {
    "psr-4": {
      "MediaMonks\\Crawler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\MediaMonks\\": "tests/"
    }
  },

  "extra": {
    "symfony": {
      "require": "4.4"
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
